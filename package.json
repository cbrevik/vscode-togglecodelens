{
    "name": "vscode-togglecodelens",
    "displayName": "Toggle CodeLens",
    "description": "Toggle editor.codeLens item in settings.json",
    "version": "1.0.1",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/cmlewis89/vscode-togglecodelens/issues"
    },
    "homepage": "https://github.com/cmlewis89/vscode-togglecodelens",
    "repository": {
        "type": "git",
        "url": "https://github.com/cmlewis89/vscode-togglecodelens"
    },
    "publisher": "cmlewis89",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.toggleCodelens"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.toggleCodelens",
                "title": "Toggle CodeLens"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^1.6.2",
        "vscode": "0.10.x"
    }
}